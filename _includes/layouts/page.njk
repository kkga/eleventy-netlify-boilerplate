---
layout: layouts/base.njk
section: page
---

<header>
  <h1>{{ title }}</h1>

  {% if subtext %}
  <small>
    {{ subtext }}. Updated
    <time datetime="{{ page.date | machineDate }}">{{
      page.date | readableDate
    }}</time
    >.
  </small>
  {% endif %}
</header>

{{ layoutContent | safe }}
